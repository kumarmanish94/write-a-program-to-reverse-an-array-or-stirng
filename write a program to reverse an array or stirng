public class RevArray
{
static void revArray(int a[]){
int start=0, end=5;
int a;
while(start<=end){
a[start];
a[start]= a[end];
a[end]=0
start++;
end--;
}
for(int i=0; i<a.length; i++){
System.out.print(a[i]+"");
}
}
public static void main(String[]args){
int[]a=new int[]{1,2,3,4,5,6};
revArray(a);
}
}
